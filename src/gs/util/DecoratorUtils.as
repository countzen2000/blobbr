package gs.util{		import flash.utils.Dictionary;		/**	 * The DecoratorUtils class contains helper methods	 * required to build chain-able decorators	 * (gs.display.decorators).	 * 	 * <script src="http://mint.codeendeavor.com/?js" type="text/javascript"></script>	 */	public class DecoratorUtils 	{				/**		 * Build the methods lookup from an		 * array of strings - which are method		 * names.		 * 		 * @param methds An array of strings - which are method names.		 */		public static function buildMethods(methds:Array):Dictionary		{			var methods:Dictionary=new Dictionary();			var method:String;			for each(method in methds)methods[method]=true;			return methods;		}				/**		 * Build the properties lookup from an		 * array of strings - which are property		 * names.		 * 		 * @param prps An array of strings - which are property names.		 */		public static function buildProps(prps:Array):Dictionary		{			var props:Dictionary=new Dictionary();			var prop:String;			for each(prop in prps)props[prop]=true;			return props;		}	}}